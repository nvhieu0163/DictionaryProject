<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<HBox fx:id="resultBox" alignment="CENTER_LEFT" prefHeight="45.0" prefWidth="340.0"
      style="-fx-border-style: hidden hidden solid hidden; -fx-border-color: #cacaca; -fx-border-width: 0.5;"
      xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Text fx:id="content" strokeType="OUTSIDE" strokeWidth="0.0" text="Word content">
            <HBox.margin>
                <Insets left="10.0" right="5.0"/>
            </HBox.margin>
            <font>
                <Font name="System Bold" size="14.0"/>
            </font>
        </Text>
        <Text fx:id="posTag" fill="#0b73c8" strokeType="OUTSIDE" strokeWidth="0.0" text="POSTag">
            <font>
                <Font name="System Italic" size="14.0"/>
            </font>
        </Text>
    </children>
    <opaqueInsets>
        <Insets/>
    </opaqueInsets>
    <cursor>
        <Cursor fx:constant="HAND"/>
    </cursor>
</HBox>
